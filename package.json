    "lucide-react": "^0.460.0",
    "pg": "^8.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.7.0",
    "remix-auth-form": "^1.5.0",
    "tailwind-merge": "^2.5.4",
